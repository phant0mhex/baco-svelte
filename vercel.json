{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api-proxy/(.*)",
      "destination": "https://mgljaheyimizrydazrxh.supabase.co/$1"
    }
  ],

  "headers": [
    {
      "source": "/api-proxy/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          
          "value": "https://baco-inky.vercel.app, *"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS, PATCH"
        },
        {
          "key": "Access-Control-Allow-Headers",
         
          "value": "Authorization, Content-Type, apikey, x-client-info, x-supabase-api-version" 
        }
      ]
    }
  ]
}
